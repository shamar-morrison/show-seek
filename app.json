{
  "expo": {
    "name": "ShowSeek",
    "slug": "showseek",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "showseek",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.horizon.showseek",
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "We'll notify you when your favorite movies are releasing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "app.horizon.showseek",
      "permissions": ["NOTIFICATIONS", "android.permission.SCHEDULE_EXACT_ALARM"],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "showseek"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#E50914"
        }
      ],
      "./withWidgetManifestFix.js",
      "./withSharedDataPackage.js",
      "react-native-iap",
      "expo-updates",
      "react-native-google-auth",
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "search_icon": {
              "foregroundImage": "./assets/images/shortcuts/search_icon.png",
              "backgroundColor": "#000000"
            },
            "discover_icon": {
              "foregroundImage": "./assets/images/shortcuts/discover_icon.png",
              "backgroundColor": "#000000"
            },
            "library_icon": {
              "foregroundImage": "./assets/images/shortcuts/library_icon.png",
              "backgroundColor": "#000000"
            },
            "watchlist_icon": {
              "foregroundImage": "./assets/images/shortcuts/watchlist_icon.png",
              "backgroundColor": "#000000"
            }
          }
        }
      ],
      [
        "@bittingz/expo-widgets",
        {
          "android": {
            "src": "./src/native-widgets/android",
            "widgets": [
              {
                "name": "UpcomingMoviesWidgetProvider",
                "resourceName": "@xml/upcoming_movies_widget_info"
              },
              {
                "name": "UpcomingTVWidgetProvider",
                "resourceName": "@xml/upcoming_tv_widget_info"
              },
              {
                "name": "WatchlistWidgetProvider",
                "resourceName": "@xml/watchlist_widget_info"
              }
            ]
          }
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#E50914",
      "androidMode": "default",
      "androidCollapsedTitle": "New releases soon!"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73837620-7f10-473e-aba7-b67e800737ea"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/73837620-7f10-473e-aba7-b67e800737ea"
    }
  }
}
